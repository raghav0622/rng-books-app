{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".vscode": true,
    "public": true,
    "app/favicon.ico": true,
    ".eslintrc.json": true,
    ".gitignore": true,
    ".prettierignore": true,
    "next-env.d.ts": true,
    "next.config.mjs": true,
    "package-lock.json": true,
    "node_modules": true,
    "rng-forms": true,
    ".next": true,
    ".prettierrc": true,
    "postcss.config.js": true,
    "app/layout.tsx": true,
    "app/loading.tsx": true,
    "app/page.tsx": true,
    "app/globals.css": true,
    "app/auth": true,
    "components/AppLogo.tsx": true,
    "components/ToggleDarkMode.tsx": true,
    "components/UserMenu.tsx": true,
    "providers/ColorModeProvider.tsx": true,
    "providers/RootProvider.tsx": true,
    "providers/ThemeProvider.tsx": true,
    "providers/UserProvider.tsx": true
  },
  "hide-files.files": [
    ".vscode",
    "public",
    "app/favicon.ico",
    ".eslintrc.json",
    ".gitignore",
    ".prettierignore",
    "next-env.d.ts",
    "next.config.mjs",
    "package-lock.json",
    "node_modules",
    "rng-forms",
    ".next",
    ".prettierrc",
    "postcss.config.js",
    "app/layout.tsx",
    "app/loading.tsx",
    "app/page.tsx",
    "app/globals.css",
    "app/auth",
    "components/AppLogo.tsx",
    "components/ToggleDarkMode.tsx",
    "components/UserMenu.tsx",
    "providers/ColorModeProvider.tsx",
    "providers/RootProvider.tsx",
    "providers/ThemeProvider.tsx",
    "providers/UserProvider.tsx"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  }
}
