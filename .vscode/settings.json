{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "package-lock.json": true,
    ".next": true,
    "node_modules": true,
    "public": true,
    ".gitignore": true,
    ".prettierignore": true,
    ".prettierrc": true,
    "next-env.d.ts": true,
    ".eslintrc.json": true,
    "postcss.config.js": true,
    ".vscode": true,
    "app/UserMenu.tsx": true,
    "app/theme.ts": true,
    "app/notificiation.module.css": true,
    "app/not-found.tsx": true,
    "app/loading.tsx": true,
    "app/globals.css": true,
    "app/favicon.ico": true,
    "app/AppLayout.tsx": true,
    "app/AppProviders.tsx": true,
    "app/Authorization.tsx": true,
    "app/layout-client.tsx": true,
    "app/layout.tsx": true,
    "app/page.tsx": true,
    "app/auth": true
  },
  "hide-files.files": [
    "package-lock.json",
    ".next",
    "node_modules",
    "public",
    ".gitignore",
    ".prettierignore",
    ".prettierrc",
    "next-env.d.ts",
    ".eslintrc.json",
    "postcss.config.js",
    ".vscode",
    "app/UserMenu.tsx",
    "app/theme.ts",
    "app/notificiation.module.css",
    "app/not-found.tsx",
    "app/loading.tsx",
    "app/globals.css",
    "app/favicon.ico",
    "app/AppLayout.tsx",
    "app/AppProviders.tsx",
    "app/Authorization.tsx",
    "app/layout-client.tsx",
    "app/layout.tsx",
    "app/page.tsx",
    "app/auth"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  }
}
